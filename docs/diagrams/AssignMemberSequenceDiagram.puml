@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ClubBookParser" as ClubBookParser LOGIC_COLOR
participant ":AssignEventCommandParser" as AssignEventCommandParser LOGIC_COLOR
participant "d:AssignEventCommand" as AssignEventCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("assignEvent \n m/John Doe e/Orientation")
activate LogicManager

LogicManager -> ClubBookParser : parseCommand("assignEvent \nm/John Doe e/Orientation")
activate ClubBookParser

create AssignEventCommandParser
ClubBookParser -> AssignEventCommandParser
activate AssignEventCommandParser

AssignEventCommandParser --> ClubBookParser
deactivate AssignEventCommandParser

ClubBookParser -> AssignEventCommandParser : parse("m/John Doe e/Orientation")
activate AssignEventCommandParser

create AssignEventCommand
AssignEventCommandParser -> AssignEventCommand:
activate AssignEventCommand

AssignEventCommand --> AssignEventCommandParser :
deactivate AssignEventCommand

AssignEventCommandParser --> ClubBookParser : d
deactivate AssignEventCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AssignEventCommandParser -[hidden]-> ClubBookParser
destroy AssignEventCommandParser

ClubBookParser --> LogicManager : d
deactivate ClubBookParser

LogicManager -> AssignEventCommand : execute(m)
activate AssignEventCommand

AssignEventCommand -> Model : assignEvent(John Doe, Orientation)
activate Model

Model --> AssignEventCommand
deactivate

create CommandResult
AssignEventCommand -> CommandResult
activate CommandResult

CommandResult --> AssignEventCommand
deactivate CommandResult

AssignEventCommand --> LogicManager : r
deactivate AssignEventCommand

[<--LogicManager
deactivate LogicManager
@enduml
